{
  "name": "@perdieminc/time-to-unix",
  "version": "1.0.0",
  "description": "Convert date/time components with timezone support to Unix timestamps",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build",
    "lint": "eslint src tests --ext .ts",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\""
  },
  "keywords": [
    "unix",
    "timestamp",
    "timezone",
    "date",
    "time",
    "converter",
    "utc"
  ],
  "author": "Per Diem",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/perdiem/unix-time-converter.git"
  },
  "bugs": {
    "url": "https://github.com/perdiem/unix-time-converter/issues"
  },
  "homepage": "https://github.com/perdiem/unix-time-converter#readme",
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=14.0.0"
  }
}